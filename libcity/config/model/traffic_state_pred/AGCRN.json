{
  "embed_dim": 30,
  "rnn_units": 128,
  "num_layers": 2,
  "cheb_order": 4,
  "add_apt": true,
  "graph_type": "geo",
  "dtw_topk": 10,
  "bidir_adj_mx": true,

  "scaler": "standard",
  "load_external": false,
  "normal_external": false,
  "ext_scaler": "none",
  "add_time_in_day": false,
  "add_day_in_week": false,

  "max_epoch": 1000,
  "learner": "adam",
  "learning_rate": 0.001,
  "lr_decay": false,
  "clip_grad_norm": false,
  "use_early_stop": true,
  "patience": 50
}
